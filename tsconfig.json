{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "moduleResolution": "bundler",
    "lib": ["es2020", "dom"],
    "strict": true,
    "noEmit": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "types": ["deno.ns"],
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["node_modules", "dist", "svg2png.wasm"]
}
